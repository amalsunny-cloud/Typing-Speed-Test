{
  "version": 2,
  "builds": [
    {
      "src": "**/*.ts",
      "use": "@vercel/typescript"     
    },
    {
      "src": "index.html",           
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "images/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}